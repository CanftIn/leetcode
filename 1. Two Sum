class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        int i = 0;
        int j = 0;
        for(int a = 0; a < nums.size(); a++){
            i = nums[a];
            for(int b = 0; b < nums.size(); b++) {
                j = nums[b];
                if((i + j) == target){
                    if(a != b){
                        vector<int> v;
                        v.push_back(a);
                        v.push_back(b);
                        return v;
                    }

                }
            }
        }
    }
};
